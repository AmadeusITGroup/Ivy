{
	"compilerOptions": {
		"module": "es6",
		"target": "es5",
		"noImplicitAny": false,
		"sourceMap": true,
		"noUnusedParameters": false,
		"noImplicitThis": true,
		"diagnostics": true,
		"listFiles": true,
		"pretty": true,
		"moduleResolution": "node",
		"noEmitOnError": false,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"experimentalDecorators": true,
		"allowUnusedLabels": true,
		"lib":[
			"es2015", "dom"
		]
	},
	"include": [
		"src/**/*.ts"
  	],
	"exclude": [
		"src/test/diffmatchpatch.js"
	]
}